    {
      "name": "Post data",
      "event": [
        {
          "listen": "test",
          "script": {
            "id": "38be8195-c6e3-40d2-a60d-ce318ef499b4",
            "exec": [
              "pm.test(\"Successful POST request\", function () {",
              "    pm.expect(pm.response.code).to.be.oneOf([200, 201]);",
              "});",
              ""
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
      "id": "b59e9b2d-6784-40c4-83b0-d87a281cb6db",
      "request": {
        "method": "POST",
        "header": [],
        "body": {
          "mode": "raw",
          "raw": "{\n\t\"userId\": 1,\n    \"id\": 101,\n    \"title\": \"some data\",\n    \"body\": \"some text soome text some test\"\n}",
          "options": {
            "raw": {
              "language": "json"
            }
          }
        },
        "url": {
          "raw": "{{baseURL}}/posts",
          "host": [
            "{{baseURL}}"
          ],
          "path": [
            "posts"
          ]
        },
        "description": "This is a POST request, submitting data to an API via the request body. This request submits JSON data, and the data is reflected in the response.\n\nA successful POST request typically returns a `200 OK` or `201 Created` response code."
      },
      "response": []
    },
    {
      "name": "Get Post based on PostId",
      "event": [
        {
          "listen": "test",
          "script": {
            "id": "1b54fabe-9590-44a6-b27e-b4ab0f7989fc",
            "exec": [
              "pm.test(\"Response has required fields\", function() {\r",
              "    const response = pm.response.json();\r",
              "\r",
              "    pm.expect(response).to.have.property('userId');\r",
              "    pm.expect(response).to.have.property('id');\r",
              "    pm.expect(response).to.have.property('title');\r",
              "    pm.expect(response).to.have.property('body');\r",
              "});\r",
              "\r",
              "pm.test(\"Response structure is correct\", function() {\r",
              "    const response = pm.response.json();\r",
              "\r",
              "    pm.expect(response.userId).to.be.a('number');\r",
              "    pm.expect(response.id).to.be.a('number');\r",
              "    pm.expect(response.title).to.be.a('string');\r",
              "    pm.expect(response.body).to.be.a('string');\r",
              "});\r",
              "\r",
              "pm.test(\"Response contains expected data\", function () {\r",
              "    const response = pm.response.json();\r",
              "\r",
              "    pm.expect(response.userId).to.equal(1);\r",
              "    pm.expect(response.id).to.equal(1);\r",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
      "id": "dd9bf85d-a656-42bc-8793-68c3276da41b",
      "request": {
        "auth": {
          "type": "noauth"
        },
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{baseURL}}/posts/:postId",
          "host": [
            "{{baseURL}}"
          ],
          "path": [
            "posts",
            ":postId"
          ],
          "variable": [
            {
              "key": "postId",
              "value": "1"
            }
          ]
        }
      },
